{
  "name": "@inspectr/inspectr",
  "version": "1.2.0",
  "description": "Inspectr - Capture and inspect HTTP requests/responses with a built-in Inspectr App.",
  "author": "Tim Haselaars",
  "homepage": "https://inspectr.dev",
  "bugs": "https://github.com/inspectr-hq/inspectr/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inspectr-hq/inspectr.git"
  },
  "bin": {
    "inspectr": "./bin/inspectr"
  },
  "files": [],
  "scripts": {
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall",
    "test": "echo 'test completed'",
    "release": "npm publish --access public"
  },
  "goBinary": {
    "name": "inspectr",
    "path": "./bin",
