  },
  "dependencies": {
    "@inspectr/go-npm": "^0.1.0"
  },
  "keywords": [
    "inspectr",
    "inspector",
    "proxy",
    "middleware",
    "viewer",
    "logger",
    "request",
    "response",
    "webhooks",
    "SSE"
  ]
}
